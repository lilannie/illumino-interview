    "react-dom": "^15.3.2",
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^6.2.2",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.26.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.2.0",
    "nodemon": "^1.11.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.13"
